<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ca.ucalgary.gui.budget.BudgetController">
   <children>
      <AnchorPane fx:id="incomePane" layoutX="-1.0" layoutY="-2.0" prefHeight="403.0" prefWidth="364.0">
         <children>
            <Label layoutX="25.0" layoutY="24.0" prefHeight="32.0" prefWidth="151.0" text="Enter Monthly Income Amount">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <TextField fx:id="incomeField" layoutX="26.0" layoutY="66.0" promptText="eg 10000" />
            <Label fx:id="incomeErrorLabel" layoutX="86.0" layoutY="236.0" prefHeight="18.0" prefWidth="191.0" textFill="RED" />
            <Label fx:id="incomeSuccessLabel" layoutX="86.0" layoutY="277.0" prefHeight="18.0" prefWidth="191.0" textFill="#00ff04" />
            <Button fx:id="enterNewIncomeButton" layoutX="75.0" layoutY="153.0" mnemonicParsing="false" onAction="#enterNewIncome" text="Set" />
            <Button fx:id="returnFromIncomeButton" layoutX="203.0" layoutY="153.0" mnemonicParsing="false" onAction="#returnToMenuFromIncome" text="Back" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
