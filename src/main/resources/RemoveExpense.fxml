<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ca.ucalgary.gui.budget.BudgetController">
   <children>
      <AnchorPane fx:id="removePane" layoutX="-1.0" layoutY="-2.0" prefHeight="403.0" prefWidth="364.0">
         <children>
            <ChoiceBox fx:id="expenseRemoveChoice" layoutX="182.0" layoutY="107.0" prefWidth="150.0" />
            <Label layoutX="8.0" layoutY="110.0" prefHeight="20.0" prefWidth="162.0" text="Select Expense to Remove">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Button fx:id="removeButton" layoutX="71.0" layoutY="189.0" mnemonicParsing="false" onAction="#removeExpenseFromBudget" text="Remove" />
            <Button fx:id="returnRemoveButton" layoutX="205.0" layoutY="189.0" mnemonicParsing="false" onAction="#returnToMenuFromRemove" text="Back" />
         </children></AnchorPane>
   </children>
</AnchorPane>
